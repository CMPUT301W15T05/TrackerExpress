<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    

    <TextView 
        android:id="@+id/tv_main_claim_list_name"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:textSize="30sp"
        android:focusableInTouchMode="false"
		android:clickable="false"
		android:focusable="false" />
    
    <TextView
        android:id="@+id/tv_main_claim_list_isIncomplete"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:textColor="@color/error_color"
        android:focusableInTouchMode="false"
		android:clickable="false"
		android:focusable="false" />
    
    <TextView
        android:id="@+id/tv_main_claim_list_incomplete_expenses"
        android:visibility="gone"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="@string/incompleteExpenses"
        android:textColor="@color/error_color"
        android:focusableInTouchMode="false"
		android:clickable="false"
		android:focusable="false" />
    
    <TextView 
        android:id="@+id/tv_main_claim_list_status"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:focusableInTouchMode="false"
		android:clickable="false"
		android:focusable="false" />
    <TextView
        android:id="@+id/tv_main_claim_list_destinations"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:focusableInTouchMode="false"
		android:clickable="false"
		android:focusable="false" />
    <TextView
        android:id="@+id/tv_main_claim_list_amounts"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:focusableInTouchMode="false"
		android:clickable="false"
		android:focusable="false" />
    <TextView
        android:id="@+id/tv_main_claim_list_tags"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:focusableInTouchMode="false"
		android:clickable="false"
		android:focusable="false" />

    <LinearLayout 
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">
        
        <TextView
       		android:id="@+id/tv_main_claim_list_start"
        	android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:focusableInTouchMode="false"
			android:clickable="false"
			android:focusable="false" />
        
        <TextView
       		android:id="@+id/tv_main_claim_list_to"
        	android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:text="@string/to"
	        android:focusableInTouchMode="false"
			android:clickable="false"
			android:focusable="false" />
        
        <TextView
       		android:id="@+id/tv_main_claim_list_end"
        	android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:focusableInTouchMode="false"
			android:clickable="false"
			android:focusable="false" />
        
    </LinearLayout>    
    <ImageView
        android:id="@+id/iv_distance"
        android:layout_width="fill_parent"
        android:layout_height="12sp"
        android:visible="false">
    </ImageView>
</LinearLayout>
